
import math
def main():
 # Отримання 4 невід'ємних цілих чисел від користувача
 a = int(input("Введіть a: "))
 b = int(input("Введіть b: "))
 c = int(input("Введіть c: "))
 d = int(input("Введіть d: "))


 # Перевірка на невід'ємність
 if a < 0 or b < 0 or c < 0 or d < 0:
   print("Всі числа повинні бути невід'ємними!")
   return


 # Отримання дійсного числа x від користувача
 x = float(input("Введіть x: "))


 # Обчислення значення Y
 y = math.sqrt(x) + a * x + b * x + c


 # Виведення результату
 print(f"Y = {y}")


if __name__ == "__main__":
 main()
