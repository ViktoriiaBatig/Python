# 1. Задання початкових значень
var_int = 13
var_float = 8.4
var_str = "No"

# 2. Збільшення var_int в 3.5 рази і зв'язування з big_int
big_int = var_int * 3.5

# 3. Зменшення var_float на одиницю
var_float -= 1

# 4. Розділення var_int на var_float та big_int на var_float
result1 = var_int / var_float
result2 = big_int / var_float

# 5. Зміна значення var_str
var_str = var_str * 2 + "Yes" * 3

# 6. Виведення значень змінних
print("var_int:", var_int)
print("var_float:", var_float)
print("var_str:", var_str)
print("big_int:", big_int)
print("Result of var_int / var_float:", result1)
print("Result of big_int / var_float:", result2)
